<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">
    <!--    to enable annotation use this syntax -->
    <context:annotation-config/>
    <bean class="autowired.autowiredusingannotation.AutowiredAnnotationParent" name="autowiredAnnotationParent">
        <property name="workingOfMachine" value="Enhance Power"/>
        <property name="nameofMachine" value="regulator"/>
    </bean>
    <bean class="autowired.autowiredusingannotation.AutowiredUsingAnnotationClass" name="autowiredAnnotationBean"/>

</beans>