<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:p="http://www.springframework.org/schema/p"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean class="com.example.propertyinjection.Student" name="Student" p:studentAddress="delhi" p:studentId="1"
          p:studentName="Vimal Singh"/>
    <bean class="emp.Emp" name="emp">
        <property name="empName" value="vimal Singh"/>
        <property name="empSubject">
            <list>
                <value>Math</value>
                <value>Science</value>
                <value>Computer</value>
            </list>
        </property>
        <property name="marks">
            <set>
                <value>90</value>
                <value>80</value>
                <value>95</value>
            </set>
        </property>
        <property name="marksheet">
            <map>
                <entry key="Math" value="90"/>
                <entry key="Science" value="80"/>
                <entry key="Computer" value="95"/>
            </map>
        </property>
    </bean>
    <bean class="aref.Vehicle" name="vehicle" p:vehicleType="Sedan"/>
    <bean class="aref.Car" name="car">
        <property name="name" value="GM"/>
        <property name="vehicle" ref="vehicle"/>
    </bean>


</beans>